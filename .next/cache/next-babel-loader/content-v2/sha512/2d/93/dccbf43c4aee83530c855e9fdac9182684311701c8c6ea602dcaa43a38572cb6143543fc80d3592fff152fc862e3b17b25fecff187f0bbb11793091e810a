  }\n\n\n  function togglePlay(){\n    setIsPlaying(!isPlaying);\n    }\n  function setPlayingSate(state:boolean){\n        setIsPlaying(state);\n    }\n  return (\n    <PlayerContext.Provider \n    value={{\n        episodeList,\n        currentEpisodeIndex,\n        play,\n        playList,\n        isPlaying,\n        togglePlay,\n        setPlayingSate\n        }}\n        >\n\n        {children}\n\n    </PlayerContext.Provider>\n\n    )\n}"]},"metadata":{},"sourceType":"module"}